{
    "core_system": {
        "$reference": {
            "registry": {"id": "complex_service"}
        },
        "database": {
            "$reference": {
                "registry": {"id": "database_service"}
            },
            "host": "core.db.example.com",
            "port": 5432,
            "database_name": "core_system"
        },
        "user_service": {
            "$reference": {
                "registry": {"id": "user_service"}
            },
            "database": {
                "$reference": {
                    "registry": {"id": "database_service"}
                },
                "host": "users.db.example.com",
                "port": 5432,
                "database_name": "users"
            },
            "cache_enabled": true
        },
        "config": {
            "plugin_enabled": "true"
        }
    },
    "plugins": [
        {
            "$reference": {
                "registry": {"id": "simple_service"}
            },
            "name": "auth_plugin"
        },
        {
            "$reference": {
                "registry": {"id": "simple_service"}
            },
            "name": "logging_plugin"
        },
        {
            "$reference": {
                "registry": {"id": "simple_service"}
            },
            "name": "metrics_plugin"
        }
    ]
}