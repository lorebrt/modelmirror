{
    "primary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            },
            "instance": "primary_db"
        },
        "host": "localhost",
        "port": 5432,
        "database_name": "app_dev"
    },
    "secondary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            }
        },
        "host": "localhost",
        "port": 5433,
        "database_name": "app_dev_secondary"
    },
    "user_service": {
        "$reference": {
            "registry": {
                "schema": "user_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "cache_enabled": false
    },
    "microservices": [
        {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "dev_service_1"
        }
    ],
    "service_registry": {
        "test": {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "test_service"
        }
    },
    "orchestrator": {
        "$reference": {
            "registry": {
                "schema": "complex_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "user_service": {
            "$reference": {
                "registry": {
                    "schema": "user_service",
                    "version": "1.0.0"
                }
            },
            "database": "$primary_db",
            "cache_enabled": true
        },
        "config": {
            "debug": "true"
        }
    },
    "environment": "development",
    "debug_mode": true
}