{
    "primary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            },
            "instance": "primary_db"
        },
        "host": "composition.db.example.com",
        "port": 5432,
        "database_name": "composition"
    },
    "secondary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            }
        },
        "host": "composition-secondary.db.example.com",
        "port": 5432,
        "database_name": "composition_secondary"
    },
    "user_service": {
        "$reference": {
            "registry": {
                "schema": "user_service",
                "version": "1.0.0"
            },
            "instance": "main_user_service"
        },
        "database": "$primary_db",
        "cache_enabled": true
    },
    "microservices": [
        {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "composition_service"
        }
    ],
    "service_registry": {
        "main": {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "main_service"
        }
    },
    "orchestrator": {
        "$reference": {
            "registry": {
                "schema": "complex_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "user_service": "$main_user_service",
        "config": {
            "composition": "true"
        }
    },
    "environment": "composition_test",
    "debug_mode": false
}