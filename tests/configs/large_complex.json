{
    "primary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            },
            "instance": "primary_db"
        },
        "host": "large.db.example.com",
        "port": 5432,
        "database_name": "large_app"
    },
    "secondary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            }
        },
        "host": "large-secondary.db.example.com",
        "port": 5432,
        "database_name": "large_secondary"
    },
    "user_service": {
        "$reference": {
            "registry": {
                "schema": "user_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "cache_enabled": true
    },
    "microservices": [
        {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "service_1"
        },
        {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "service_2"
        }
    ],
    "service_registry": {
        "main": {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "main_service"
        }
    },
    "orchestrator": {
        "$reference": {
            "registry": {
                "schema": "complex_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "user_service": {
            "$reference": {
                "registry": {
                    "schema": "user_service",
                    "version": "1.0.0"
                }
            },
            "database": "$primary_db",
            "cache_enabled": true
        },
        "config": {
            "large": "true"
        }
    },
    "environment": "performance_test",
    "debug_mode": false
}