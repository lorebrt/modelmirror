{
    "primary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            },
            "instance": "primary_db"
        },
        "host": "prod.db.example.com",
        "port": 5432,
        "database_name": "app_prod"
    },
    "secondary_database": {
        "$reference": {
            "registry": {
                "schema": "database_service",
                "version": "1.0.0"
            }
        },
        "host": "prod-secondary.db.example.com",
        "port": 5432,
        "database_name": "app_prod_secondary"
    },
    "user_service": {
        "$reference": {
            "registry": {
                "schema": "user_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "cache_enabled": true
    },
    "microservices": [
        {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "prod_service_1"
        }
    ],
    "service_registry": {
        "main": {
            "$reference": {
                "registry": {
                    "schema": "simple_service",
                    "version": "1.0.0"
                }
            },
            "name": "main_service"
        }
    },
    "orchestrator": {
        "$reference": {
            "registry": {
                "schema": "complex_service",
                "version": "1.0.0"
            }
        },
        "database": "$primary_db",
        "user_service": {
            "$reference": {
                "registry": {
                    "schema": "user_service",
                    "version": "1.0.0"
                }
            },
            "database": "$primary_db",
            "cache_enabled": true
        },
        "config": {
            "debug": "false"
        }
    },
    "environment": "production",
    "debug_mode": false
}